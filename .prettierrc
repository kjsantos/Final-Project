{
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-organize-imports",
		"prettier-plugin-css-order",
		"prettier-plugin-tailwindcss"
	],
	"pluginSearchDirs": false,
	"trailingComma": "es5",
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"arrowParens": "always",
	"bracketSameLine": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
